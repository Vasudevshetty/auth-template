{
  "name": "auth-template",
  "version": "1.0.0",
  "description": "A comprehensive authentication service template with TypeScript, Express, and JWT",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "format": "prettier --write 'src/**/*.ts'"
